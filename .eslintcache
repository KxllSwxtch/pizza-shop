s = useStyles();\n\n  const handleAddToCart = () => {\n    const cartItem = { id, name, price, quantity: 1 };\n    dispatch({ type: ADD_TO_CART, cartItem });\n    openAlert('Item added to cart');\n  };\n\n  const formattedPrice = (\n    <CurrencyConverter from=\"USD\" to={currentCurrency} value={currentPrice} />\n  );\n\n  return (\n    <Card className={classes.root} elevation={6}>\n      <CardActionArea>\n        <CardMedia\n          className={classes.media}\n          component=\"img\"\n          src={imageURL}\n          title={name}\n        />\n        <CardContent className={classes.cardContent}>\n          <Typography gutterBottom variant=\"h5\" component=\"h2\">\n            {name}\n          </Typography>\n          <Typography variant=\"body2\" color=\"textSecondary\" component=\"p\">\n            Price: {formattedPrice}\n          </Typography>\n        </CardContent>\n      </CardActionArea>\n      <CardActions className={classes.cardActions}>\n        <Button\n          size=\"small\"\n          color=\"primary\"\n          variant=\"contained\"\n          onClick={handleAddToCart}\n        >\n          Add to cart\n        </Button>\n        <Button\n          size=\"small\"\n          color=\"secondary\"\n          variant=\"outlined\"\n          onClick={() => dispatch({ type: GET_PIZZA_DETAILS, pizzaID: id })}\n        >\n          <Link to={`/pizza/${id}`} className={classes.link}>\n            See Details\n          </Link>\n        </Button>\n      </CardActions>\n    </Card>\n  );\n}\n\nexport default PizzaListItem;\n","/Users/dmitriyshin.io/Desktop/Work/Challenges/Innoscripta/pizza-shop/src/redux/reducers/userReducer.js",[],"/Users/dmitriyshin.io/Desktop/Work/Challenges/Innoscripta/pizza-shop/src/pages/index.js",[],"/Users/dmitriyshin.io/Desktop/Work/Challenges/Innoscripta/pizza-shop/src/pages/Checkout.js",[],"/Users/dmitriyshin.io/Desktop/Work/Challenges/Innoscripta/pizza-shop/src/Routes.js",[],"/Users/dmitriyshin.io/Desktop/Work/Challenges/